<!doctype html>
<html>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
<script src="https://unpkg.com/leaflet@1.0.3/dist/leaflet.js"></script>
<script src="js/leaflet-geojson-selector.js"></script>
<script src="js/mapScript.js"></script>

  <head>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/unsemantic/1.2.3/unsemantic-grid-responsive-tablet.css">
    <link rel="stylesheet" type="text/css" href="css/main.css">
    <link rel="stylesheet" type="text/css" href="css/home.css">
    <link href="https://fonts.googleapis.com/css?family=Archivo+Narrow|Poiret+One" rel="stylesheet">
    <link rel="stylesheet" href="https://unpkg.com/leaflet@1.0.3/dist/leaflet.css"/>
    <link rel="stylesheet" href="css/leaflet-geojson-selector.css" />
    <meta name="viewport" content="width=device-width">
  </head>

  <body>


<!-- HEADER STARTS -->

  <header class="custom-grid-container">
    <div>
      <a href="home.ejs"  id="logoLink">
        <img src="images/logo.png" id="logoImage">
      </a>
    </div>
  </header>


<!-- MAIN STARTS -->

  <main id="Page" class="grid-container">
    <div id="optionsBar">
      <button class="sideButton" onclick="menu_toggle()">â˜°</button>
      <button class="loginButton" onclick="login_menu()">Profile</button>
      <div class="profileMenu">
         <a href="loginPage.ejs" onclick="return !window.open(this.href, 'somesite', 'width=500,height=500')"
         target="_blank">
         <button>Login</button></a>
         <a href="registerPage.ejs" onclick="return !window.open(this.href, 'somesite', 'width=500,height=500')"
         target="_blank"><button>Register</button> </a>
      </div>
      <div class="searchbox" id='SiteNavigation'>
        <span class="icon"> <i class="searchContainer"></i></span>
        <input type="search" id="search" placeholder="Search..." />
      </div>
     </div>

    <div class="sidebarContainer grid-30 mobile-grid-100" id="sidebar">
      <h4 id="dietary">Dietary Restrictions</h4>
       <div class="Dietary">
          <button>
           Meat
          </button>

          <button>
           Gluten
          </button>

          <button>
           Dairy
          </button>
        </div>

      <h4 id="continental">Continental Restrictions</h4>
        <div class="Continental">
          <button type="button">
           Asia
          </button>

          <button type="button">
           Africa
          </button>

          <button type="button">
           North America
          </button>

          <button type="button">
           South America
          </button>

          <button type="button">
           Antartica
          </button>

          <button type="button">
           Europe
          </button>

          <button type="button">
           Oceania
          </button>
        </div>

      <h4 id="time">Time Restrictions</h4>
        <div class="Time">
          <button type="button">
           Breakfast
          </button>

          <button type="button">
           Brunch
          </button>

          <button type="button">
           Lunch
          </button>

          <button type="button">
           Dinner
          </button>

          <button type="button" id="lastButton">
           Snack
          </button>
        </div>
    </div>


<!--SITE NAVIGATION-->



<!--MAP IMAGE-->

    <div class="mapContainer grid-70 mobile-grid-100" id="mapid">
      <!-- <img src="images/map.png" id="interactivemap"> -->
      <!-- Map goes here -->
    </div>
  </main>


<!-- FOOTER STARTS -->

    <footer id="PageFooter" class="custom-grid-container">
      <div class="grid-100">
          <a href="legal.ejs"><button>Legal</button></a>
          <a href="about.ejs"><button class="middleButton">About</button></a>
          <a href="contact.ejs"><button>Contact</button></a>
      </div>
    </footer>


<!--JS-->

    <script src="js/scripting.js"></script>
