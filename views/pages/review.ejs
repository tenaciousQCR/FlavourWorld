<!doctype html>
<html>
  <head>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/unsemantic/1.2.3/unsemantic-grid-responsive-tablet.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="css/main.css">
    <link rel="stylesheet" type="text/css" href="css/review.css">
    <link href="https://fonts.googleapis.com/css?family=Archivo+Narrow|Poiret+One" rel="stylesheet">
  </head>
  <body>


<!-- HEADER STARTS -->

    <header class="custom-grid-container">
      <% include ../partials/header %>
    </header>


<!-- MAIN STARTS -->

    <main id="Page" class="grid-container">
      <div id="optionsBar">
        <% include ../partials/profileMenu %>
      </div>


<!-- TEXTBOX -->

      <form action="/reviewdish" method="POST">
          <div class="container">
            <label for="reviewbox"><b>Your Review</b></label>
            <input type="text" placeholder="What did you think of this dish?" tabindex="1" name="reviewbox" required>
          </div>


  <!-- STAR RATINGS -->

        <div>
          <b>Taste</b>
          <p>
            <img src="images/star-empty-single.png" id="star11" class="star" tabindex="2">
            <img src="images/star-empty-single.png" id="star12" class="star" tabindex="3">
            <img src="images/star-empty-single.png" id="star13" class="star" tabindex="4">
            <img src="images/star-empty-single.png" id="star14" class="star" tabindex="5">
            <img src="images/star-empty-single.png" id="star15" class="star" tabindex="6">
          </p>
        </div>
        <div>
          <b>Cost</b>
          <p>
            <img src="images/star-empty-single.png" id="star21" class="star" tabindex="7">
            <img src="images/star-empty-single.png" id="star22" class="star" tabindex="8">
            <img src="images/star-empty-single.png" id="star23" class="star" tabindex="9">
            <img src="images/star-empty-single.png" id="star24" class="star" tabindex="10">
            <img src="images/star-empty-single.png" id="star25" class="star" tabindex="11">
          </p>
        </div>
        <div>
          <b>Ease of Preparation</b>
          <p>
            <img src="images/star-empty-single.png" id="star31" class="star" tabindex="12">
            <img src="images/star-empty-single.png" id="star32" class="star" tabindex="13">
            <img src="images/star-empty-single.png" id="star33" class="star" tabindex="14">
            <img src="images/star-empty-single.png" id="star34" class="star" tabindex="15">
            <img src="images/star-empty-single.png" id="star35" class="star" tabindex="16">
          </p>
        </div>
        <div>
          <p>
            <button class="submitButton" tabindex="17">Submit</button>
          </p>
        </div>
      </form>
    </main>


<!-- FOOTER STARTS -->

    <footer id="PageFooter" class="custom-grid-container">
      <% include ../partials/footer %>
    </footer>


<!--JS-->

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <script src="js/starrating.js"></script>
    <script src="js/scripting.js"></script>
  </body>
</html>
