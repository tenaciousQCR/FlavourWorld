<!doctype html>
<html>
  <head>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/unsemantic/1.2.3/unsemantic-grid-responsive-tablet.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="css/main.css">
    <link rel="stylesheet" type="text/css" href="css/review.css">
    <link href="https://fonts.googleapis.com/css?family=Archivo+Narrow|Poiret+One" rel="stylesheet">
  </head>
  <body>


    <!-- HEADER STARTS -->

    <header class="custom-grid-container">
      <% include ../partials/header %>
    </header>


<!-- MAIN STARTS -->

    <main id="Page" class="grid-container">
      <div id="optionsBar">
        <button class="loginButton" onclick="login_menu()">Profile</button>
        <div class="profileMenu">
           <a href="loginPage">
           <button>Login</button></a>
           <a href="registerPage">
           <button>Register</button> </a>
        </div>
       </div>
      <div>
        <div class="container">
          <label for="uname"><b>Username</b></label>
          <input type="text" placeholder="Enter Username" name="uname" required>
        <!-- TEXTBOX -->
      </div>
      <div>
        <p>
          <img src="images/star-empty-single.png" id="star1-1">
          <img src="images/star-empty-single.png" id="star1-2">
          <img src="images/star-empty-single.png" id="star1-3">
          <img src="images/star-empty-single.png" id="star1-4">
          <img src="images/star-empty-single.png" id="star1-5">
        </p>
      </div>
      <div>
        <p>
          <img src="images/star-empty-single.png" id="star2-1">
          <img src="images/star-empty-single.png" id="star2-2">
          <img src="images/star-empty-single.png" id="star2-3">
          <img src="images/star-empty-single.png" id="star2-4">
          <img src="images/star-empty-single.png" id="star2-5">
        </p>
      </div>
      <div>
        <p>
          <img src="images/star-empty-single.png" id="star3-1">
          <img src="images/star-empty-single.png" id="star3-2">
          <img src="images/star-empty-single.png" id="star3-3">
          <img src="images/star-empty-single.png" id="star3-4">
          <img src="images/star-empty-single.png" id="star3-5">
        </p>
      </div>
      <div>
        <p>
          <button id="submitButton">Submit</button>
        </p>
      </div>
    </main>


<!-- FOOTER STARTS -->

    <footer id="PageFooter" class="custom-grid-container">
      <% include ../partials/footer %>
    </footer>


<!--JS-->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <script src="js/starrating.js"></script>
    <script src="js/scripting.js"></script>
  </body>
</html>
